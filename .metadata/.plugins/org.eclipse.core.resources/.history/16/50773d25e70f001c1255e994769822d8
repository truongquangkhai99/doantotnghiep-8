import org.springframework.stereotype.Repository;

import com.tutv.entity.User;

/**
 * UserDAO
 */

@Repository
public interface UserDAO {
	
	/**
	 * getUser
	 *
	 * @param id
	 * @param lang
	 * @return list of users
	 */
	public User getUser(Integer id);

}
